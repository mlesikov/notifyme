<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="NotifyMe">

  <inherits name="com.google.gwt.user.User"/>
  <inherits name="com.google.gwt.inject.Inject"/>
  
  <entry-point class="com.clouway.notifyme.client.NotifyMe"/>
  <servlet path="/ConnectionService" class="com.clouway.notifyme.server.ConnectionServiceImpl"/>
  <servlet path="/SubscriptionService" class="com.clouway.notifyme.server.SubscriptionServiceImpl"/>

  <source path="client" />
  <source path="shared" />
  <source path="server" />

</module>